import { JsonRpcProvider } from 'ethers';

// Connect to the Ethereum network
const provider = new JsonRpcProvider("https://base-mainnet.g.alchemy.com/v2/odqj1Inc4Ob7xhGONE2GHkauhylEXMSA");

// Get block by number
const blockNumber = "latest";
const block = await provider.getBlock(blockNumber);

console.log(block);
